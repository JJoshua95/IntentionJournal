<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms" 
		xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
		xmlns:mypages="clr-namespace:IntentionJournal;assembly=IntentionJournal"
		xmlns:local="clr-namespace:IntentionJournal;assembly=IntentionJournal" 
		x:Class="IntentionJournal.MainTabContainer"
		Title = "Intention Journal">
	<TabbedPage.Children>
		<NavigationPage Title = "Home" Icon="settings.png">
			<x:Arguments>
				<local:HomePage/>
			</x:Arguments>
		</NavigationPage>
		<NavigationPage Title = "Browse" Icon="settings.png">
			<x:Arguments>
				<local:BrowseMoodsPage/>
			</x:Arguments>
		</NavigationPage>
		<NavigationPage Title = "Write" Icon="settings.png">
			<x:Arguments>
				<local:AddEntryPage/>
			</x:Arguments>
		</NavigationPage>
		<NavigationPage Title = "Tree" Icon="settings.png">
			<x:Arguments>
				<local:Tree/>
			</x:Arguments>
		</NavigationPage>
		<NavigationPage Title = "Settings" Icon="settings.png">
			<x:Arguments>
				<local:Settings/>
			</x:Arguments>
		</NavigationPage>
	</TabbedPage.Children>
</TabbedPage>

